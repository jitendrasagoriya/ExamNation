        <ImageButton
            android:id="@+id/btnFindMe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_72x72" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>